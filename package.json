{
  "name": "ololog",
  "version": "1.0.2",
  "description": "Platform-agnostic logging / ANSI styles / shows code locations / pretty prints objects",
  "main": "ololog",
  "scripts": {
    "test": "mocha --reporter spec",
    "autotest": "mocha --reporter spec --watch",
    "build": "browserify --debug ./ololog.browser.js > ./build/ololog.browser.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xpl/ololog.git"
  },
  "keywords": [
    "logging",
    "log",
    "ansi",
    "pretty-print",
    "stacktrace",
    "aspect-oriented-programming",
    "chrome-devtools",
    "debug",
    "debugging-tool",
    "console-log",
    "terminal-colors",
    "tty"
  ],
  "author": "Vitaly Gordon <rocket.mind@gmail.com>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/xpl/ololog/issues"
  },
  "homepage": "https://github.com/xpl/ololog",
  "devDependencies": {
    "browserify": "^14.3.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "ansicolor": "^1.0.68",
    "pipez": "0.0.9",
    "printable-characters": "^1.0.18",
    "stacktracey": "^1.0.44",
    "string.bullet": "^1.0.9",
    "string.ify": "^1.0.34"
  }
}
