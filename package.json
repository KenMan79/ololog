{
  "name": "ololog",
  "version": "0.0.31",
  "description": "Platform-agnostic logging / ANSI styles / shows code locations / pretty prints objects",
  "main": "ololog",
  "scripts": {
    "test": "mocha --reporter spec --watch",
    "build": "browserify ./ololog.browser.js > ./build/ololog.browser.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xpl/ololog.git"
  },
  "keywords": [
    "logging",
    "log",
    "ansi",
    "pretty-print",
    "stacktrace",
    "aspect-oriented-programming",
    "chrome-devtools",
    "debug",
    "debugging-tool",
    "console-log",
    "terminal-colors",
    "tty"
  ],
  "author": "Vitaly Gordon <rocket.mind@gmail.com>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/xpl/ololog/issues"
  },
  "homepage": "https://github.com/xpl/ololog",
  "devDependencies": {
    "browserify": "^14.3.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "ansicolor": "*",
    "stacktracey": "*",
    "string.bullet": "*",
    "string.ify": "*",
    "printable-characters": "*",
    "pipez": "*"
  }
}
