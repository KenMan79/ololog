{
  "name": "ololog",
  "version": "1.0.8",
  "description": "Platform-agnostic logging with colors in terminals & browsers / shows call locations / pretty prints objects and stacktraces / smart newline and indentation handling",
  "main": "build/ololog.es5",
  "scripts": {
    "build": "npm run lint && npm run lint-test && npm run babel && npm run browserify && npm run test && npm run test-es5",
    "lint": "eslint ololog.js",
    "lint-test": "eslint test.js",
    "test": "env OLOLOG_TEST_FILE='./ololog' nyc --reporter=html --reporter=text mocha --reporter spec",
    "test-es5": "env OLOLOG_TEST_FILE='./build/ololog.es5' mocha --reporter spec",
    "autotest": "env OLOLOG_TEST_FILE='./ololog' mocha --reporter spec --watch",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "babel": "babel ololog.js --source-maps inline --out-file ./build/ololog.es5.js",
    "browserify": "browserify --debug ./ololog.browser.js > ./build/ololog.browser.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xpl/ololog.git"
  },
  "keywords": [
